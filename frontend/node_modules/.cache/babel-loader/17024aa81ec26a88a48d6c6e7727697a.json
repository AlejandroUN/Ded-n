O,EAAE;AACD,IAAA,KADC,mBACO;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEJ,qBAAqB,CAAC,KAAtB,CAA4B;AAChC;AACA,kBAAA,KAAK,EAAE,KAAI,CAAC,KAFoB;AAEb;AACnB,kBAAA,QAAQ,EAAE,KAAI,CAAC,QAHiB,CAGP;;AAHO,iBAA5B,CAFI;;AAAA;AAMN;AACJ,gBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,kBAAA,IAAI,EAAE;AAAR,iBAAlB;;AACA,gBAAA,IAAI,CAAC,IAAL,CAAU;AACR,kBAAA,IAAI,EAAE,SADE;AAER,kBAAA,KAAK,EAAE,OAFC;AAGR,kBAAA,IAAI,EAAE;AAHE,iBAAV;AARU;AAAA;;AAAA;AAAA;AAAA;AAcV,gBAAA,KAAI,CAAC,KAAL,GAAa,YAAM,QAAN,CAAe,IAAf,CAAoB,KAAjC;AACA,gBAAA,IAAI,CAAC,IAAL,CAAU;AACR,kBAAA,IAAI,EAAE,OADE;AAER,kBAAA,KAAK,EAAE,OAFC;AAGR,kBAAA,IAAI,EAAE,KAAI,CAAC;AAHH,iBAAV;;AAfU;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBb;AAtBM;AAPI,CAAf","sourcesContent":["<template>\n  <div class=\"container col-xl-12 col-xxl-8 py-3\">\n    <div class=\"row align-items-center g-lg-5\">\n      <header class=\"font\">\n        <router-link to=\"/\"\n          ><img src=\"../assets/logo.png\" height=\"50\"\n        /></router-link>\n        <!--<img src=\"../assets/logo.png\" height=\"50\"/>-->\n        <nav class=\"nav justify-content-center float-md-end right\">\n          <router-link to=\"/\"\n            ><button\n              class=\"w-100 btn btn-lg btn-primary color_fuente color2\"\n              type=\"submit\"\n            >\n              Inicio\n            </button></router-link\n          >\n          <router-link to=\"\"\n            ><button\n              class=\"btn btn-lg btn-primary color_fuente color2\"\n              type=\"submit\"\n            >\n              ¡Dónanos!\n            </button></router-link\n          >\n        </nav>\n      </header>\n\n      <div\n        class=\"\n          modal modal-signin\n          position-static\n          d-block\n          py-5\n          bg\n          iniciar_sesion_blue_container\n        \"\n        tabindex=\"-1\"\n        role=\"dialog\"\n        id=\"modalSignin\"\n      >\n        <div class=\"float-none cajitaMIBT\" role=\"document\">\n          <div class=\"modal-content rounded-5 shadow\">\n            <div class=\"modal-header p-5 pb-4 border-bottom-0 fondo3 font\">\n              <h2 class=\"fw-bold mb-0\">Test MBTI - Relaciones Personales</h2>\n            </div>\n\n            <div class=\"modal-body p-5 pt-0 fondo3\">\n\n              <div class=\"form-floating mb-3\">\n                <label for=\"floatingInput\">Correo electronico</label>\n                <input\n                  type=\"email\"\n                  class=\"form-control\"\n                  id=\"floatingInput\"\n                  placeholder=\"name@example.com\"\n                  v-model=\"email\"\n                />\n              </div>\n\n              <div class=\"form-group mb-3\">\n                <label class=\"custom-label col-md-12\">¿Cual?</label>\n                <input\n                  class=\"form-control col-12\"\n                  placeholder=\"name@example.com\"\n                  v-model=\"email\"\n                />                \n              </div>\n\n              <div class=\"form-floating mb-3\">\n                <input\n                  type=\"password\"\n                  class=\"form-control\"\n                  id=\"floatingPassword\"\n                  placeholder=\"Password\"\n                  v-model=\"password\"\n                />\n                <label for=\"floatingPassword\">Contraseña</label>\n              </div>\n\n              <button\n                class=\"mb-2 btn btn-sm rounded-4 btn-primary floatr right color\"\n                type=\"submit\"\n                v-on:click=\"login\"\n              >\n                Entrar\n              </button>\n              <small class=\"text-muted\"\n                ><router-link to=\"/recuperarContrasena\"\n                  >¿Has olvidado tu contraseña?</router-link\n                ></small\n              >\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthenticationService from \"@/services/AuthenticationService\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        await AuthenticationService.login({\n          // eslint-disable-line no-mixed-spaces-and-tabs\n          email: this.email, // eslint-disable-line no-mixed-spaces-and-tabs\n          password: this.password, // eslint-disable-line no-mixed-spaces-and-tabs\n        }); // eslint-disable-line no-mixed-spaces-and-tabs\n        this.$router.push({ path: \"/entrarPerfil\" });\n        Swal.fire({\n          icon: \"success\",\n          title: \"Login\",\n          text: \"El usuario ha entrado correctamente\",\n        });\n      } catch (error) {\n        this.error = error.response.data.error;\n        Swal.fire({\n          icon: \"error\",\n          title: \"Error\",\n          text: this.error,\n        });\n      }\n    },\n  },\n};\n</script>\n\n<style>\n* {\n  font-family: AvGard;\n}\n\n@font-face {\n  font-family: AvGard;\n  src: url(AVGARDN_2.TTF);\n}\n\n.cajitaMIBT {\n  padding-left: 105px;\n  padding-right: 105px;\n}\n.iniciar_sesion_blue_container {\n  background-color: #008b8f !important;\n}\n.fondo3 {\n  background-color: #faefe8 !important;\n}\n</style>\n"],"sourceRoot":""}]}