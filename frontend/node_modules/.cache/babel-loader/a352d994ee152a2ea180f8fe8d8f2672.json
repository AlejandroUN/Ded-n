              </button>\r\n              <small class=\"text-muted\"\r\n                ><router-link to=\"/recuperarContrasena\"\r\n                  >¿Has olvidado tu contraseña?</router-link\r\n                ></small\r\n              >\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!--\r\n\r\n    <div id=\"app\">\r\n      \r\n    </div>\r\n    -->\r\n</template>\r\n<script>\r\nimport AuthenticationService from \"@/services/AuthenticationService\";\r\nimport Swal from \"sweetalert2\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        //We keep track of the backend response\r\n\r\n        let caracteres_unicos = \"|°¬!#$%&/()='?¿¡´¨+*~}]`[{^,;:<>\";\r\n        let new_email = \"\";\r\n\r\n        for (let i1 = 0; i1 < caracteres_unicos.length; ++i1) {\r\n          for (let i2 = 0; i2 < this.email.length; ++i2) {\r\n            if (caracteres_unicos.charAt(i1) == this.email.charAt(i2)) {\r\n              new_email = this.email.replace(caracteres_unicos.charAt(i1), \"\");\r\n              this.email = new_email;\r\n            }\r\n          }\r\n        }\r\n\r\n        for (let i1 = 0; i1 < caracteres_unicos.length; ++i1) {\r\n          for (let i2 = 0; i2 < this.password.length; ++i2) {\r\n            if (caracteres_unicos.charAt(i1) == this.password.charAt(i2)) {\r\n              new_email = this.password.replace(\r\n                caracteres_unicos.charAt(i1),\r\n                \"\"\r\n              );\r\n              this.password = new_email;\r\n            }\r\n          }\r\n        }\r\n\r\n        console.log(this.password);\r\n\r\n        const response = await AuthenticationService.login({\r\n          // eslint-disable-line no-mixed-spaces-and-tabs\r\n          email: this.email, // eslint-disable-line no-mixed-spaces-and-tabs\r\n          password: this.password, // eslint-disable-line no-mixed-spaces-and-tabs\r\n        }); // eslint-disable-line no-mixed-spaces-and-tabs\r\n        //This is going to call our method setToken in the store file\r\n        //which is gonna call our mutation setToken\r\n        //which is gonna update our state to token\r\n        this.$store.dispatch(\"setToken\", response.data.token);\r\n        //console.log(this.email)\r\n        //console.log(response.data.user)\r\n        //console.log(response.data.token)\r\n        //And the same with token\r\n        this.$store.dispatch(\"setUser\", response.data.user);\r\n        this.$router.push({ path: \"/introBigFive\" });\r\n        Swal.fire({\r\n          icon: \"success\",\r\n          title: \"Login\",\r\n          text: \"El usuario ha entrado correctamente\",\r\n        });\r\n      } catch (error) {\r\n        this.error = error.response.data.error;\r\n        Swal.fire({\r\n          icon: \"error\",\r\n          title: \"Error\",\r\n          text: this.error,\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.iniciar_sesion_blue_container {\r\n  background-color: #008b8f !important;\r\n}\r\n.fondo3 {\r\n  background-color: #faefe8 !important;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}