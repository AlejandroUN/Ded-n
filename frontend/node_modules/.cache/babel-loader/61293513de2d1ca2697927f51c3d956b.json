AA,QAAQ,EAAE,EAJL;AAKL,MAAA,IAAI,EAAE,IAAI,IAAJ,GAAW,WAAX,GAAyB,MAAzB,CAAgC,CAAhC,EAAmC,EAAnC,CALD;AAML,MAAA,KAAK,EAAE,IAAI,IAAJ,GAAW,WAAX,GAAyB,MAAzB,CAAgC,CAAhC,EAAmC,EAAnC,CANF;AAOL,MAAA,MAAM,EAAE,EAPH;AAQL,MAAA,WAAW,EAAE,EARR;AASL,MAAA,WAAW,EAAE,EATR;AAUL,MAAA,gBAAgB,EAAE;AAVb,KAAP;AAYD,GAdY;AAeb,EAAA,OAAO,EAAE;AACD,IAAA,OADC,qBACS;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACV,gBAAA,OADU,GACA,IAAI,IAAJ,EADA;AAEd,gBAAA,OAAO,CAAC,QAAR,CAAiB,OAAO,CAAC,QAAR,KAAqB,GAAtC;AACI,gBAAA,QAHU,GAGC,IAAI,IAAJ,CAAS,KAAI,CAAC,IAAd,CAHD;;AAAA,oBAKT,KAAI,CAAC,KAAL,CAAW,QAAX,CAAoB,YAApB,CALS;AAAA;AAAA;AAAA;;AAMZ,gBAAA,KAAK,CAAC,gCAAD,CAAL;AANY;AAAA;;AAAA;AAAA,sBAOH,KAAI,CAAC,QAAL,CAAc,MAAd,GAAuB,CAPpB;AAAA;AAAA;AAAA;;AAQZ,gBAAA,KAAK,CAAC,8CAAD,CAAL;AARY;AAAA;;AAAA;AAAA,sBASH,OAAM,GAAI,QATP;AAAA;AAAA;AAAA;;AAUZ,gBAAA,KAAK,CAAC,sDAAD,CAAL;AAVY;AAAA;;AAAA;AAAA;AAAA;AAAA,uBAaJ,qBAAqB,CAAC,QAAtB,CAA+B;AACnC;AACA,kBAAA,IAAI,EAAE,KAAI,CAAC,OAFwB;AAGnC,kBAAA,KAAK,EAAE,KAAI,CAAC,KAHuB;AAGhB;AACnB,kBAAA,IAAI,EAAE,KAAI,CAAC,QAJwB;AAKnC,kBAAA,QAAQ,EAAE,KAAI,CAAC,QALoB;AAKV;AACzB,kBAAA,KAAK,EAAE,KAAI,CAAC,IANuB;AAOnC,kBAAA,MAAM,EAAE,KAAI,CAAC,MAPsB;AAQnC,kBAAA,WAAW,EAAE,KAAI,CAAC,WARiB;AASnC,kBAAA,SAAS,EAAE,KAAI,CAAC,WATmB;AAUnC,kBAAA,cAAc,EAAE,KAAI,CAAC;AAVc,iBAA/B,CAbI;;AAAA;AAwBN;AACJ,gBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB;AAAE,kBAAA,IAAI,EAAE;AAAR,iBAAlB;;AAzBU;AAAA;;AAAA;AAAA;AAAA;AA2BV,gBAAA,KAAI,CAAC,KAAL,GAAa,YAAM,QAAN,CAAe,IAAf,CAAoB,KAAjC;;AA3BU;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8Bf,KA/BM;AAgCP,IAAA,UAhCO,wBAgCM;AACX,UAAI,KAAK,MAAL,IAAe,MAAnB,EAA2B;AACzB,eAAO,IAAP;AACF,OAFA,MAEO;AACL,eAAO,KAAP;AACF;AACD,KAtCM;AAuCP,IAAA,eAvCO,6BAuCW;AAChB,UAAI,KAAK,WAAL,IAAoB,MAAxB,EAAgC;AAC9B,eAAO,IAAP;AACF,OAFA,MAEO;AACL,eAAO,KAAP;AACF;AACD,KA7CM;AA+CP,IAAA,YAAY,EAAE,sBAAU,GAAV,EAAe;AAC3B,MAAA,GAAE,GAAI,GAAE,GAAI,GAAJ,GAAU,MAAM,CAAC,KAAzB;AACA,UAAI,QAAO,GAAI,GAAG,CAAC,KAAJ,GAAY,GAAG,CAAC,KAAhB,GAAwB,GAAG,CAAC,OAA3C;;AACA,UAAI,QAAO,GAAI,CAAX,KAAiB,QAAO,GAAI,EAAX,IAAiB,QAAO,GAAI,GAA7C,CAAJ,EAAuD;AACrD,QAAA,GAAG,CAAC,cAAJ;AACF,OAFA,MAEO;AACL,eAAO,IAAP;AACF;AACD;AAvDM;AAfI,CAAf","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"container\">\n      <h2>Crea tu cuenta</h2>\n    </div>\n    <div class=\"container\">\n      <div>\n        <input\n          type=\"text\"\n          name=\"nombres\"\n          v-model=\"nombres\"\n          placeholder=\"Nombres y Apellidos\"\n          maxlength=\"50\"\n        />\n      </div>\n      <div>\n        <input\n          type=\"text\"\n          name=\"email\"\n          v-model=\"email\"\n          placeholder=\"Correo electrónico\"\n        />\n      </div>\n      <div>\n        <input\n          type=\"text\"\n          name=\"new_user\"\n          v-model=\"new_user\"\n          placeholder=\"Nombre usuario nuevo\"\n        />\n      </div>\n      <div>\n        <input\n          type=\"password\"\n          name=\"password\"\n          v-model=\"password\"\n          placeholder=\"Contraseña nueva\"\n          minlength=\"8\"\n          maxlength=\"20\"\n          v-on:keypress=\"isImprimible(event)\"\n        />\n      </div>\n      <div>\n        <form>\n          <div><label>Fecha de nacimiento:</label></div>\n          <div><input type=\"date\" :max=\"today\" v-model=\"born\" /></div>\n        </form>\n      </div>\n      <div>\n        <div><label>Genero y orientación sexual:</label></div>\n        <div>\n          <select v-model=\"gender\">\n            <option>Hombre cisgenero</option>\n            <option>Mujer cisgenero</option>\n            <option>Hombre transgenero</option>\n            <option>Mujer transgenero</option>\n            <option>Hombre</option>\n            <option>Mujer</option>\n            <option>Persona no binaria</option>\n            <option>Genero fluido</option>\n            <option>Intersexual</option>\n            <option>Otro</option>\n            <option>Prefiero no contestar</option>\n          </select>\n          <input\n            v-show=\"otroGenero()\"\n            type=\"text\"\n            v-model=\"nuevoGenero\"\n            placeholder=\"Ingrese genero\"\n          />\n          <select v-model=\"orientacion\">\n            <option>Heterosexual</option>\n            <option>Homosexual</option>\n            <option>Bisexual</option>\n            <option>Pansexual</option>\n            <option>Asexual</option>\n            <option>Antrosexual</option>\n            <option>Demisexual</option>\n            <option>Otro</option>\n            <option>Prefiero no contestar</option>\n          </select>\n          <input\n            v-show=\"otraOrientacion()\"\n            type=\"text\"\n            v-model=\"nuevaOrientacion\"\n            placeholder=\"Ingrese orientación\"\n          />\n        </div>\n      </div>\n      <div>\n        <button v-on:click=\"checked\">Crear cuenta</button>\n      </div>\n      {{ nombres }} - {{ email }} - {{ new_user }} - {{ password }} -\n      {{ born }} - {{ gender }} - {{ orientacion }} - {{ today }}\n    </div>\n  </div>\n</template>\n\n<script>\nimport AuthenticationService from \"@/services/AuthenticationService\";\nexport default {\n  data() {\n    return {\n      nombres: \"\",\n      email: \"\",\n      new_user: \"\",\n      password: \"\",\n      born: new Date().toISOString().substr(0, 10),\n      today: new Date().toISOString().substr(0, 10),\n      gender: \"\",\n      orientacion: \"\",\n      nuevoGenero: \"\",\n      nuevaOrientacion: \"\",\n    };\n  },\n  methods: {\n    async checked() {\n      var dateMax = new Date();\n      dateMax.setMonth(dateMax.getMonth() - 168);\n      var dateBorn = new Date(this.born);\n\n      if (!this.email.includes(\"@gmail.com\")) {\n        alert(\"El correo indicado no es gmail\");\n      } else if (this.password.length < 8) {\n        alert(\"La contraseña debe tener más de 8 caracteres\");\n      } else if (dateMax < dateBorn) {\n        alert(\"El usuario es menor de 14 años, no puede registrarse\");\n      } else {\n        try {\n          await AuthenticationService.register({\n            // eslint-disable-line no-mixed-spaces-and-tabs\n            name: this.nombres,\n            email: this.email, // eslint-disable-line no-mixed-spaces-and-tabs\n            nick: this.new_user,\n            password: this.password, // eslint-disable-line no-mixed-spaces-and-tabs\n            birth: this.born,\n            gender: this.gender,\n            orientation: this.orientacion,\n            newgender: this.nuevoGenero,\n            neworientation: this.nuevaOrientacion            \n          }); // eslint-disable-line no-mixed-spaces-and-tabs\n          this.$router.push({ path: \"/entrarPerfil\" });\n        } catch (error) {\n          this.error = error.response.data.error;\n        }\n      }\n    },\n    otroGenero() {\n      if (this.gender == \"Otro\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n    otraOrientacion() {\n      if (this.orientacion == \"Otro\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n\n    isImprimible: function (evt) {\n      evt = evt ? evt : window.event;\n      var charCode = evt.which ? evt.which : evt.keyCode;\n      if (charCode > 3 && (charCode < 32 || charCode > 126)) {\n        evt.preventDefault();\n      } else {\n        return true;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}